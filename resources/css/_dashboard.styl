.dashboard
    display flex
    flex-direction column
    align-items center
    justify-content center
    background center no-repeat #bbb
    background-size cover
    height 100vh
    width auto
    h3
        font-size 24px
        font-weight bold
        background #fff
        padding 5px 30px
        border-radius 5px
        margin-bottom 10px
        opacity .7
        box-shadow inset 0 0 3px #000
    ul
        li
            display inline-block
        
